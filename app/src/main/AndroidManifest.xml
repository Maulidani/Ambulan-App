<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:dist="http://schemas.android.com/apk/distribution"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.skripsi.ambulanapp"
    android:targetSandboxVersion="1"
    tools:ignore="UnusedAttribute" >

    <uses-permission android:name="android.permission.INTERNET" />

    <permission
        android:name="com.skripsi.ambulanapp.permission.MAPS_RECEIVE"
        android:protectionLevel="signature" />

    <uses-feature
        android:glEsVersion="0x00020000"
        android:required="true" />

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <dist:module dist:instant="true" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AmbulanApp"
        android:usesCleartextTraffic="true" >
        <activity
            android:name=".ui.admin.AddEditCarActivity"
            android:exported="false" />
        <activity
            android:name=".ui.admin.LoginAdminActivity"
            android:exported="false" />
        <activity
            android:name=".ui.admin.MainAdminActivity"
            android:exported="true" >
        </activity>
        <activity
            android:name=".ui.driver.OrderHistoryActivity"
            android:exported="false" />
        <activity
            android:name=".ui.driver.ProfileActivity"
            android:exported="false" />
        <activity android:name=".ui.admin.AddEditDriverActivity" />
        <activity android:name=".ui.driver.LoginActivity" />
        <activity android:name=".ui.driver.MainDriverActivity" />
        <activity android:name=".ui.customer.MainCustomerActivity" />
        <activity
            android:name=".ui.SplashScreenActivity"
            android:exported="true" >

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_map_api" />
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
    </application>

</manifest>